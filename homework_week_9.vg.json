{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Unemployment Rate (2019)",
  "width": 800,
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/Sravan-Krsna/FIT3179-Assignment-2/main/JSON/ne_110m.json",
    "format": {
          "type": "topojson",
          "feature": "ne_110m_admin_0_countries"
        }
  },
  "transform": [{
      "lookup": "properties.NAME",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Sravan-Krsna/FIT3179-Assignment-2/main/Data/economic_freedom_index2019_data.csv"
        },
        "key": "Country Name",
        "fields": ["Unemployment (%)"]
      }
    }   
  ],
  "projection": {
    "type": "equirectangular"
  },
  "mark": {
    "type": "geoshape"
    },
  "encoding": {
    "color": {
      "field": "Unemployment (%)",
      "type": "quantitative",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "Country Name"},
      {"field": "Unemployment (%)", "type": "quantitative"}
    ]
  }

}

 